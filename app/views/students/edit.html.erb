<%= form_with url: "/students/#{@student_id}", method: :patch, local: true do |form| %>
<%= form.label :student_name %>
<%= form.text_field :student_name %>
<%= form.label :age %>
<%= form.text_field :age %>
<%= form.label :frl %>
<%= form.text_field :frl%>
<%= form.submit 'Update Student' %>
<% end %>
